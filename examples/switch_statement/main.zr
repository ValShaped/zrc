#include <stdio.zh>

fn main() -> i32 {
    let day: i32 = 1;
    switch (day) {
        1 => printf("Monday\n");
        2 => printf("Tuesday\n");
        3 => printf("Wednesday\n");
        default => printf("Other day\n");
    }
    
    // Test with u8 scrutinee and {int} cases
    let small_num: u8 = 5;
    switch (small_num) {
        5 => printf("Five\n");
        10 => printf("Ten\n");
        default => printf("Other\n");
    }
    
    return 0;
}
